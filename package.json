{
  "name": "jxnblk",
  "version": "1.4.0",
  "description": "SVG avatar for Jxnblk",
  "main": "index.js",
  "bin": {
    "jxnblk": "./cli.js"
  },
  "scripts": {
    "512": "npx repng src/Avatar.js -f avatar-512.png -w 512 -h 512 -p '{\"size\":512}'",
    "640": "npx repng src/Avatar.js -f avatar-640.png -w 640 -h 640 -p '{\"size\":640}'",
    "768": "npx repng src/Avatar.js -f avatar-768.png -w 768 -h 768 -p '{\"size\":768}'",
    "test": "./cli.js",
    "prepare": "npm run 512 && npm run 640 && npm run 768",
    "svg": "node svg.js",
    "dev": "npx repng src/Avatar.js --dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jxnblk/avatar.git"
  },
  "keywords": [
    "avatar",
    "jxnblk"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jxnblk/avatar/issues"
  },
  "homepage": "https://github.com/jxnblk/avatar",
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "repng": "^3.0.0"
  },
  "dependencies": {
    "boxen": "^3.1.0",
    "chalk": "^2.4.2"
  }
}
